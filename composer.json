{
    "name": "schnafte/kutjera",
    "description": "This library provides a service to quickly search through JSON data by providing a query string. Useful for prototyping a JSON API.",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas H.",
            "email": "thomas.hannemann@gmail.com"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "morris/microdb": "~0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "5.2.*"
    },
    "autoload": {
        "psr-4": {
            "Schnafte\\Kutjera\\": "src/"
        },
        "files": [
            "src/Util/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Schnafte\\Kutjera\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
